node {

      stage('backend') {
             stages {
               // Express Build
               stage('back_build') {
                 steps {
                   sh "docker build --file backend/dockerfile --tag test ."
                 }
               }

               // Express Run
               stage('back_run') {
                 steps {
                   sh "docker run -d -p 5000:8080 test"
                 }
               }
}