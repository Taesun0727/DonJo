node {

      stage('backend') {
             stages {
               // Express Build
               stage('back_build') {
                 steps {
                    dir('backend'){
                   sh "docker build --tag test ."
                 }
                 }
               }

               // Express Run
               stage('back_run') {
                 steps {
                   sh "docker run -d -p 8080:8080 test"
                 }
               }
}