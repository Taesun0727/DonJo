pipeline {
   stage('backend') {
             stages {
               stage('backend_build') {
                 steps {
                   dir('backend') {
                     sh "docker build --tag test ."
                   }
                 }
               }

               // 스프링 Run
               stage('backend_serve') {
                 steps {
                   sh "docker run -d -p 8080:8080 --name test"
                 }
               }
               }
}
}